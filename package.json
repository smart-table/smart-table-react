  "devDependencies": {
    "preact": "^8.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rollup": "^1.12.3",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "smart-table-core": "^3.0.4",
    "smart-table-json-pointer": "^2.0.4"
